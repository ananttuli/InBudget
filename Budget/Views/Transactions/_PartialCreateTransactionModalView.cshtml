@model UpsertTransactionViewModel


@if (Model.Transaction is null)
{
    <h3>Not Found</h3>
}
else
{
    <form id="create-transaction-form" action="/Home/CreateTransaction" method="post" onsubmit="createTransaction(event)">
        @Html.AntiForgeryToken()
        <div class="row">
            <partial name="_PartialUpsertTransactionDetails" for="@Model"/>

            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                <button type="submit" class="btn btn-primary">Save changes</button>
            </div>
        </div>
    </form>
}